{
  "compilerOptions": {
    "checkJs": true,
    "module": "esnext",
    "target": "ES2017",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "**/*.js",
    "**/*.jsx",
    ".prisma/client"
  ]
}
